{
  "name": "mazey",
  "version": "3.10.2",
  "description": "A functional library for daily front-end work.",
  "main": "lib/index.cjs.js",
  "module": "lib/index.esm.js",
  "typings": "lib/index.d.ts",
  "mainEntryPointFilePath": "lib/index.d.ts",
  "directories": {
    "lib": "./lib"
  },
  "scripts": {
    "dev": "webpack serve --config ./scripts/webpack.config.dev.js --open",
    "build": "cross-env SCRIPTS_NPM_PACKAGE_DEBUG=\"close\" rollup --config ./scripts/rollup.config.js",
    "build:debug": "cross-env SCRIPTS_NPM_PACKAGE_DEBUG=\"open\" rollup --config ./scripts/rollup.config.js",
    "test": "jest",
    "test:debug": "npm run build:debug && npm run test",
    "lint:fix": "eslint ./src --ext 'ts,tsx' --fix",
    "docs": "typedoc --readme ./README.md --excludeProtected --excludePrivate --excludeInternal --hideGenerator",
    "preview": "npm run build && npm run test",
    "release": "npm run preview && cross-env SCRIPTS_NPM_PACKAGE_VERSION=$npm_package_version node ./scripts/release.js",
    "release:win": "bash scripts/env.sh && npm i && npm run release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mazeyqian/mazey.git"
  },
  "keywords": [
    "load",
    "math",
    "url",
    "style",
    "css",
    "debounce",
    "throttle",
    "trim",
    "clone",
    "json",
    "random",
    "dom",
    "hash",
    "storage",
    "cookie",
    "calculate",
    "formula",
    "browser",
    "performance",
    "pwa",
    "console",
    "function",
    "typescript"
  ],
  "author": {
    "name": "Mazey Chu",
    "email": "mazeyqian@gmail.com",
    "url": "https://blog.mazey.net/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mazeyqian/mazey/issues"
  },
  "homepage": "https://github.com/mazeyqian/mazey#readme",
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-typescript": "^7.10.4",
    "@babel/runtime": "^7.20.7",
    "@babel/runtime-corejs3": "^7.11.2",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/jquery": "^3.5.18",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "clean-webpack-plugin": "^1.0.1",
    "core-js": "^3.6.5",
    "cross-env": "^7.0.3",
    "date-fns": "^2.29.3",
    "eslint": "^8.23.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "execa": "^4.1.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.3.8",
    "jest": "^27.5.1",
    "lint-staged": "^10.0.8",
    "markdown-toc": "^1.2.0",
    "prettier": "^1.19.1",
    "prettier-eslint-cli": "^5.0.0",
    "rollup": "^2.79.1",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-cleaner": "^1.0.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "ts-loader": "^8.0.2",
    "tslib": "^2.6.2",
    "typedoc": "^0.24.8",
    "typescript": "5.1.6",
    "webpack": "^4.46.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^3.11.3"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  }
}
