{
  "name": "mazey",
  "version": "1.2.9",
  "description": "Mazey's library for front end.",
  "main": "src/index.js",
  "module": "lib/index.cjs.js",
  "directories": {
    "example": "example"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-js": "npx babel src/js/index.ori.js -o src/js/index.bundle.js && uglifyjs src/js/index.bundle.js -o src/js/index.js",
    "build": "webpack --config webpack.config.js",
    "server": "webpack-dev-server --open",
    "doc": "npx jsdoc -c ./jsdoc.config.json -d ./doc",
    "release": "npx rollup -c && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mazeyqian/mazey-ui.git"
  },
  "keywords": [
