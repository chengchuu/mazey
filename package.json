    "Vue",
    "Scss"
  ],
  "author": "MazeyQian",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mazeyqian/mazey-ui/issues"
  },
  "homepage": "https://github.com/mazeyqian/mazey-ui#readme",
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "http-server": "^0.11.1",
    "jsdoc": "^3.6.3",
    "rollup": "^1.31.1",
    "rollup-plugin-babel": "^4.3.3",
    "uglify-js": "^3.4.7",
    "uglifycss": "0.0.29"
  }
