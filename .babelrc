{
  "presets": [
    ["@babel/preset-env", {
      "targets": {
        "browsers": ["> 1%", "last 2 versions", "android>4.0"]
      }
    }],
    "@babel/typescript"
  ],
  "plugins": [
		"@babel/proposal-class-properties",
		"@babel/proposal-object-rest-spread"
	],
  "sourceType": "unambiguous"
}
